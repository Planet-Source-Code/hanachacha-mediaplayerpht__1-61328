Public bShow As Boolean
Private Sub Form_Load()
bShow = True
Me.imgVisual.Picture = LoadPicture(strPathSkin & "\Frame\PL.jpg")
    For X = 0 To pctEqua.Count - 1
        pctEqua(X).Picture = LoadPicture(strPathSkin & "\Frame\colvisual1.jpg")
    Next X
    frmMedia.timerVisual.Enabled = False
    Me.Left = frmMedia.Width + frmMedia.Left
    Me.Top = frmMedia.Top
    Me.Timer1.Enabled = True
End Sub
Private Sub Form_Unload(Cancel As Integer)
    bShow = False
    If frmMedia.mnuSpectrumC(1).Checked = True Then
        For X = 0 To frmMedia.pM.Count - 1
            frmMedia.pM(X).Visible = True
            frmMedia.p(X).Visible = True
        Next X
        frmMedia.timerVisual.Enabled = True
    End If
    If frmMedia.mnuSpectrumC(2).Checked = True Then
        For X = 0 To frmMedia.prow.Count - 1
            frmMedia.prow(X).Visible = True
            frmMedia.prowM(X).Visible = True
        Next X
        frmMedia.timerVisual.Enabled = True
    End If
    Me.Timer1.Enabled = False
    frmMedia.mnuSpectrumC(1).Enabled = True
    frmMedia.mnuSpectrumC(2).Enabled = True
    frmMedia.mnuSpectrumC(3).Checked = False
End Sub
Private Sub imgVisual_MouseDown(Button As Integer, Shift As Integer, X As Single, Y As Single)
On Error Resume Next
If Button = vbRightButton Then
    Exit Sub
        Else
    If Button = vbMiddleButton Then
    Exit Sub
        Else
    ReleaseCapture
    SendMessage Me.hWnd, &HA1, 2, 0&
    End If
    End If
End Sub
Private Sub pctMask_DblClick()
    Unload Me
